# Google API Configuration
GOOGLE_API_KEY=<YOUR_API_KEY_HERE>

# Model Configuration
MODEL_RAG=gemini-2.5-flash-lite
MODEL_SENTIMENT=gemini-2.5-flash-lite
MODEL_AGGREGATOR=gemini-2.5-flash-lite
MODEL_SAFETY_CHECK=gemini-2.5-flash-lite
MODEL_REFINER=gemini-2.5-flash-lite

# RAG & Knowledge Base Settings
RAG_MAX_PAGES=100
RAG_MAX_DEPTH=5
RAG_CHUNK_SIZE=800
RAG_CHUNK_OVERLAP=200
RAG_RETRIEVAL_K=5

# Paths & Directories
DOC_FOLDER_PATH=<YOUR_DOC_FOLDER_RELATIVE_PATH_HERE>
DOC_WEB_URL_ROOT=<YOUR_DOC_WEB_URL_ROOT_HERE>
CACHE_DIR=cache

# Database Configuration
SQL_ASYNC_DRIVER=aiosqlite
DB_NAME=vaxtalk_sessions.db

# API Retry Configuration
RETRY_ATTEMPTS=3
RETRY_INITIAL_DELAY=1
RETRY_HTTP_STATUS_CODES=429,500,503,504
